      [[1, 1], [1, 2], [1, 3]],
      [[3, 2], [4, 3]]];


      var expected = [[1, 1], [4, 2], [5, 3]];

      var result = _timeseries2.default.sumSeries(series);
      expect(result).toEqual(expected);
      done();
    });
  });
