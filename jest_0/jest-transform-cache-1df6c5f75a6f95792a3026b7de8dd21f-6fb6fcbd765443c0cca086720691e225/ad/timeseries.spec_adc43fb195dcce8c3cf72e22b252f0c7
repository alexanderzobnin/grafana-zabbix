  describe('sumSeries()', function () {
    it('should properly sum series', function (done) {
      var series = [
      [[0, 1], [1, 2], [1, 3]],
      [[2, 1], [3, 2], [4, 3]]];


      var expected = [[2, 1], [4, 2], [5, 3]];

